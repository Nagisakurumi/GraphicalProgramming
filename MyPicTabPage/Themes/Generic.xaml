<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:MyNamespace="clr-namespace:MyPicTabPage;assembly=MyPicTabPage"
    xmlns:local="clr-namespace:MyPicTabPage">
    <Style TargetType="{x:Type local:PicTabPage}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:PicTabPage}">
                    <Canvas IsItemsHost="True">
                        
                    </Canvas>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="{x:Type local:PicFunctionTabPage}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:PicFunctionTabPage}">
                    <Canvas IsItemsHost="True">

                    </Canvas>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

