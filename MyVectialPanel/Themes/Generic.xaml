<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:MyNamespace="clr-namespace:MyVectialPanel;assembly=MyVectialPanel"
    xmlns:local="clr-namespace:MyVectialPanel">
    <Style TargetType="{x:Type local:VectialPanel}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:VectialPanel}">
                    
                    <StackPanel Orientation="Vertical" IsItemsHost="True" Name="MyPanel">

                    </StackPanel>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>
