<UserControl x:Class="MyXToolsPanel.SelectTypeEnum"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MyXToolsPanel"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Name="main" Height="29.057" Width="280">
    <StackPanel Width="{Binding ElementName=main, Path=Width}" Height="{Binding ElementName=main,Path=Height}" Orientation="Horizontal">
        <TextBlock Height="{Binding ElementName=main,Path=Height}" FontSize="23" Width="147" Name="PropertyName">属性名称</TextBlock>
        <ComboBox Height="{Binding ElementName=main,Path=Height}" Width="133" Name="DataComBox" SelectionChanged="DataComBox_SelectionChanged" FontSize="22"></ComboBox>
    </StackPanel>
</UserControl>
